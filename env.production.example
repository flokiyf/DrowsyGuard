# Configuration de production pour Vercel
NODE_ENV=production

# DÃ©sactiver le mode debug en production
NEXT_PUBLIC_DEBUG_MODE=false

# Configuration MediaPipe
NEXT_PUBLIC_MEDIAPIPE_CDN=https://cdn.jsdelivr.net/npm/@mediapipe/face_mesh/

# Analytics (optionnel)
NEXT_PUBLIC_ANALYTICS_ID=

# Sentry pour le monitoring d'erreurs (optionnel)
NEXT_PUBLIC_SENTRY_DSN=

# Configuration des alertes
NEXT_PUBLIC_ENABLE_SOUND_ALERTS=true
NEXT_PUBLIC_ENABLE_VIBRATION_ALERTS=true

# Limites de performance
NEXT_PUBLIC_MAX_FPS=30
NEXT_PUBLIC_MAX_DETECTION_TIME=300000 